# Discord bot token
DISCORD_TOKEN=your_bot_token_here

# Base URL for calendar feeds, without trailing slash
BASE_URL=https://calendar.yourdomain.com

# HTTP port for the server (default 9000)
HTTP_PORT=9000

# Poll interval in minutes (default 15)
POLL_INTERVAL=15

# Data directory for JSON and ICS files (relative path)
DATA_DIR=calendars
